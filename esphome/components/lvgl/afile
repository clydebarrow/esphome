for i in animimg.py \
arc.py \
btn.py \
btnmatrix.py \
checkbox.py \
dropdown.py \
img.py \
keyboard.py \
label.py \
led.py \
line.py \
lv_bar.py \
lv_switch.py \
menu.py \
meter.py \
obj.py \
page.py \
roller.py \
slider.py \
spinbox.py \
spinner.py \
tabview.py \
textarea.py \
tileview.py
do
    tname=`basename $i .py`
    gsed "s/    async def to_code(/    @property\n    def w_type(self):\n        return lv_${tname}_t\n\n&/" \
        <$i >xfile
    mv xfile $i
done
